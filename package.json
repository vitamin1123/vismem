    "less": "^4.1.3",
    "pdfjs-dist": "^4.8.69",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.1",
    "pinyin-match": "^1.2.6",
    "tdesign-mobile-vue": "0.18.0-beta",
    "tdesign-vue-next": "^1.10.4",
    "uuid": "^9.0.0",
    "vant": "^4.9.17",
    "vue": "^3.2.47",
    "vue-pdf": "^4.3.0",
    "vue-pdf-embed": "^2.1.1",
    "vue-router": "^4.1.6"
